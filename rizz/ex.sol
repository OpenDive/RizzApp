/**
 * @dev royalty Token Validation functions
 *
 
 */
library royalties {
    bytes4 private constant _INTERFACE_ID_ERC2981 = 0x2a55205a;
    using SafeERC20 for IERC20;

    function checkRoyalties(ERC2981 _contract) internal view returns (bool) {
        return _contract.supportsInterface(_INTERFACE_ID_ERC2981);
    }

    function getRoyalty(
        ERC2981 tokenContract,
        uint256 tokenID,
        uint256 price
    ) internal returns (address, uint256) {
        address receiver;
        uint256 royalty;

        if (checkRoyalties(tokenContract)) {
            (receiver, royalty) = tokenContract.royaltyInfo(tokenID, price);
        }
        return (receiver, royalty);
    }

    function royaltyTransfer(
        address nft,
        address feeToken,
        uint256 price,
        uint256 tokenID
    ) internal returns (uint256 royalty) {
        (address recipient, uint256 _royalty) = getRoyalty(
            ERC2981(nft),
            tokenID,
            price
        );

        if (_royalty > 0 && recipient != address(0)) {
            IERC20(feeToken).safeTransfer(recipient, _royalty);
        }

        return _royalty;
    }

    function royaltyTransferFrom(
        address nft,
        address feeToken,
        address buyer,
        uint256 price,
        uint256 tokenID
    ) internal returns (uint256 royalty, bool success) {
        (address recipient, uint256 _royalty) = getRoyalty(
            ERC2981(nft),
            tokenID,
            price
        );

        if (_royalty > 0 && recipient != address(0)) {
            IERC20(feeToken).safeTransferFrom(buyer, recipient, _royalty);
            success = true;
        }

        return (_royalty, success);
    }
}

